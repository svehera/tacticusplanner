{
    "$schema": "https://json.schemastore.org/prettierrc.json",
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "bracketSameLine": true,
    "printWidth": 120,
    "endOfLine": "auto",
    "plugins": ["prettier-plugin-tailwindcss"]
}
