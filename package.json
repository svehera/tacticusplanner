  "scripts": {
    "start": "vite",
    "start-local": "vite --mode dev-local",
    "build": "bin/regenerate-onslaught-data && tsc && vite build",
    "build-dev": "bin/regenerate-onslaught-data && tsc && vite build --mode staging",
    "build-ci": "npm run format-ci && npm run lint-ci && npm run build && npm run test",
    "build-dev-ci": "npm run format-ci && npm run lint-ci && npm run build-dev && npm run test",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "analyze": "vite-bundle-visualizer -c ./vite.config.mts",
    "preview": "vite preview",
    "lint-ci": "eslint src --quiet",
    "lint": "eslint src --quiet --fix",
    "format": "prettier src --write",
    "format-ci": "prettier src --check",
    "knip": "knip",
    "spellcheck": "cspell .",
    "postinstall": "patch-package --error-on-fail"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx,css,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
